{
  "name": "selectel-storage-client",
  "version": "0.4.0",
  "description": "Node.js client for Selectel Storage",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib/* && rimraf dist/* && rimraf coverage/*",
    "pre-commit": "lint-staged",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "selectel"
  ],
  "lint-staged": {
    "*.ts*": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Akiyakov Denis<newsiberian2015@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/newsiberian/selectel-storage-client/issues"
  },
  "homepage": "https://github.com/newsiberian/selectel-storage-client#readme",
  "dependencies": {
    "got": "^9.4.0"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.0",
    "@types/got": "9.4.1",
    "@types/jest": "24.0.9",
    "@types/node": "11.9.5",
    "dotenv": "6.2.0",
    "faker": "4.1.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.2",
    "tslint": "5.13.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.test\\.ts)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "all",
    "parser": "typescript"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
